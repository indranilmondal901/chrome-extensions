{
    "name":"Color-Picker",
    "description": "A extension for picking color from website.",
    "version": "1.0",
    "manifest_version": 3,
    "background":{
        "service_worker":"background.js"
    },
    "permissions":[
        "activeTab", 
        "scripting"
    ],
    "action":{
        "default_popup": "popup.html"
    },
    "icons":{
        "16":"/images/color-picker-logo.png",
        "32":"/images/color-picker-logo.png",
        "48":"/images/color-picker-main.png"
    }
}